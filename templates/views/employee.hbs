<!DOCTYPE html>

<html>
	<head>
		<title>Employee</title>
	</head>
	<body>
		
		<table class="table">
		<thead>
			<tr>
				<th>Date</th>
				<th>Status</th>
				<th>Category</th>
				<th>Priority</th>
				<th></th> 
			</tr>
		</thead>
		<tbody>
			{{#each result}}
			<tr>
				<td>{{ this.nice_dt }}</td>
				<td>{{ this.status }}</td>
				<td>{{ this.category }}</td>
				<td>{{ this.priority }}</td>
				<td>
					<a href="/specialist/view/{{{this.id}}}">Details</a>	
				</td>
				<td>
					<a href="#" onclick="open_chat({{{this.id}}})">Chat</a>	
				</td>
			</tr>
			{{/each}}
		</tbody>
		</table>
	</body>
</html>
